{
  "require": {
    "craftcms/ckeditor": "^4.0",
    "craftcms/cms": "^5.0",
    "craftcms/mailgun": "^3.1",
    "putyourlightson/craft-dashboard-begone": "^3.0",
    "vlucas/phpdotenv": "^5.4",
    "wrav/oembed": "^3.0.8"
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.11.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
